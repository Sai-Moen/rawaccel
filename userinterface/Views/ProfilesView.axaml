<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             xmlns:lvc="using:LiveChartsCore.SkiaSharpView.Avalonia"
             xmlns:lvcore="using:LiveChartsCore"
             x:Class="userinterface.Views.ProfilesView">

  <TabControl>
    <TabItem Header="{Binding ProfileOneTitle}">
      <lvc:CartesianChart
        Series="{Binding Series}"
        XAxes="{Binding XAxes}"
        YAxes="{Binding YAxes}"
        Title="{Binding ChartTitle}"
        UpdaterThrottler="00:00:00.000"/>
    </TabItem>
    <TabItem Header="Profile2">
      This will be another profile.
    </TabItem>
  </TabControl>
</UserControl>
